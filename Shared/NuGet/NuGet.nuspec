  <files>
    <file src="..\..\lib\Zebble.OpenTok.iOS.*" target="lib\Xamarin.iOS10\" />
    <file src="..\..\lib\Zebble.OpenTok.Android.*" target="lib\MonoAndroid10\" />
    <file src="..\..\lib\Zebble.OpenTok.UWP.*" target="lib\UAP10\" />
    <file src="..\..\lib\bin\ARM\Release\Zebble.OpenTok.UWP.*" target="runtimes\win10-arm\lib\uap10.0" />
    <file src="..\..\lib\bin\x64\Release\Zebble.OpenTok.UWP.*" target="runtimes\win10-x64\lib\uap10.0" />
    <file src="..\..\lib\bin\x86\Release\Zebble.OpenTok.UWP.*" target="runtimes\win10-x86\lib\uap10.0" />
  </files>
</package>