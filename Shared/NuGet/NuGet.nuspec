<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Zebble.OpenTok</id>
    <version>1.0.2</version>
    <title> OpenTok for Zebble for Xamarin</title>
    <description>OpenTok plugin for Zebble for Xamarin.</description>
    <iconUrl>https://raw.githubusercontent.com/Geeksltd/Zebble.OpenTok/master/Shared/Icon.png</iconUrl>
    <authors>Geeks Ltd</authors>
    <owners>Geeks Ltd</owners>
    <projectUrl>https://github.com/Geeksltd/Zebble.OpenTok</projectUrl>
    <copyright>Copyright © 2020 Geeks Ltd - All rights reserved.</copyright>
    <dependencies>
      <group targetFramework="MonoAndroid10">
        <dependency id="Xamarin.OpenTok.Android" version="2.18.0" />
      </group>
      <group targetFramework="Xamarin.iOS10">
        <dependency id="Xamarin.OpenTok.iOS" version="2.18.0" />
      </group>
      <group targetFramework="UAP10"></group>
    </dependencies>
